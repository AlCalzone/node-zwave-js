	"xml": [
		{
			"id": "2201",
			"create_time": "2018-11-29 05:15:06",
			"name": "",
			"description": "",
			"xml": "<?xml version=\"1.0\" encoding=\"utf-8\"?>\r\n<Product>\r\n    <Model>ZW075-A02</Model>\r\n    <Label lang=\"en\">Smart Energy Switch 5th Edition</Label>\r\n\r\n    <CommandClasses>\r\n        <Class>\r\n            <id>0x00</id>             <!-- NO_OPERATION -->\r\n        </Class>\r\n        <Class>\r\n            <id>0x20</id>             <!-- BASIC -->\r\n        </Class>\r\n        <Class>\r\n            <id>0x22</id>             <!-- APPLICATION_STATUS -->\r\n        </Class>\r\n        <Class>\r\n            <id>0x25</id>             <!-- SWITCH_BINARY -->\r\n        </Class>\r\n        <Class>\r\n            <id>0x27</id>             <!-- SWITCH_ALL -->\r\n        </Class>\r\n        <Class>\r\n            <id>0x2B</id>             <!-- SCENE_ACTIVATION -->\r\n        </Class>\r\n        <Class>\r\n            <id>0x30</id>             <!-- SENSOR_BINARY -->\r\n        </Class>\r\n        <Class>\r\n            <id>0x32</id>             <!-- METER -->\r\n        </Class>\r\n        <Class>\r\n            <id>0x56</id>             <!-- CRC_16_ENCAP -->\r\n        </Class>\r\n        <Class>\r\n            <id>0x59</id>             <!-- ASSOCIATION_GROUP_INFO -->\r\n        </Class>\r\n        <Class>\r\n            <id>0x70</id>             <!-- CONFIGURATION -->\r\n        </Class>\r\n        <Class>\r\n            <id>0x72</id>             <!-- MANUFACTURER_SPECIFIC -->\r\n        </Class>\r\n        <Class>\r\n            <id>0x84</id>             <!-- WAKE_UP -->\r\n        </Class>\r\n        <Class>\r\n            <id>0x85</id>             <!-- ASSOCIATION -->\r\n        </Class>\r\n        <Class>\r\n            <id>0x86</id>             <!-- VERSION -->\r\n        </Class>\r\n    </CommandClasses>\r\n\r\n    <Configuration>\r\n\r\n        <Parameter>\r\n            <Index>2</Index>\r\n            <Label lang=\"en\">Blinking Behaviour</Label>\r\n            <Type>short</Type>\r\n            <Default>0</Default>\r\n            <Minimum>0</Minimum>\r\n            <Maximum>65535</Maximum>\r\n            <Size>2</Size>\r\n            <Help><![CDATA[The LSB defines the total time the device need to blink.\r\n<p>&lt;p&gt;The LSB defines the total time the device need to blink. The value if set in seconds. 1 - 255 The MSB defines the on\\/off interval of the blinking. The unit is 0.1 s. This is a double byte value.&lt;\\/p&gt;</p>\r\n            ]]></Help>\r\n        </Parameter>\r\n\r\n        <Parameter>\r\n            <Index>3</Index>\r\n            <Label lang=\"en\">Current Overload Protection</Label>\r\n            <Type>list</Type>\r\n            <Default>0</Default>\r\n            <Minimum>0</Minimum>\r\n            <Maximum>255</Maximum>\r\n            <Size>1</Size>\r\n            <Item>\r\n                <Value>0</Value>\r\n                <Label lang=\"en\">Disabled</Label>\r\n            </Item>\r\n            <Item>\r\n                <Value>1</Value>\r\n                <Label lang=\"en\">Enabled</Label>\r\n            </Item>\r\n            <Help><![CDATA[Load will be closed when the Current over runs\r\n<p>&lt;p&gt;\\u00a0Load will be closed when the Current over runs (US: 15.5A, other country: 16.2A) for more than 2 minutes.&lt;\\/p&gt;</p>\r\n            ]]></Help>\r\n        </Parameter>\r\n\r\n        <Parameter>\r\n            <Index>80</Index>\r\n            <Label lang=\"en\">Notification on Status Change</Label>\r\n            <Type>list</Type>\r\n            <Default>0</Default>\r\n            <Minimum>0</Minimum>\r\n            <Maximum>255</Maximum>\r\n            <Size>1</Size>\r\n            <Item>\r\n                <Value>0</Value>\r\n                <Label lang=\"en\">Disable</Label>\r\n            </Item>\r\n            <Item>\r\n                <Value>1</Value>\r\n                <Label lang=\"en\">Hail CC Sent</Label>\r\n            </Item>\r\n            <Item>\r\n                <Value>2</Value>\r\n                <Label lang=\"en\">Basic CC Report Sent</Label>\r\n            </Item>\r\n            <Help><![CDATA[Enable device to send notification to associated devices \r\n<p>&lt;p&gt;Enable device to send notification to associated devices (Group 1) when the state of Micro Smart Switch's load changed. DEFAULT is Disable&lt;\\/p&gt;</p>\r\n            ]]></Help>\r\n        </Parameter>\r\n\r\n        <Parameter>\r\n            <Index>90</Index>\r\n            <Label lang=\"en\">Enables\\/disables parameter 91\\/92</Label>\r\n            <Type>list</Type>\r\n            <Default>1</Default>\r\n            <Minimum>0</Minimum>\r\n            <Maximum>255</Maximum>\r\n            <Size>1</Size>\r\n            <Item>\r\n                <Value>0</Value>\r\n                <Label lang=\"en\">Disable</Label>\r\n            </Item>\r\n            <Item>\r\n                <Value>1</Value>\r\n                <Label lang=\"en\">Enable</Label>\r\n            </Item>\r\n            <Help><![CDATA[Automated sending of report triggered by minimal change of value.\r\n<p>&lt;p&gt;Users, who want a more constant monitoring of power consumption should set this value to 1. The reporting period is by default quite\\u00a0long ( approx. 35minutes)&lt;\\/p&gt;</p>\r\n            ]]></Help>\r\n        </Parameter>\r\n\r\n        <Parameter>\r\n            <Index>91</Index>\r\n            <Label lang=\"en\">Minimum Change to send Report (Watt)</Label>\r\n            <Type>short</Type>\r\n            <Default>25</Default>\r\n            <Minimum>0</Minimum>\r\n            <Maximum>32000</Maximum>\r\n            <Size>2</Size>\r\n            <Help><![CDATA[The value represents the minimum change in Wattage for a Report to be sent.\r\n<p>&lt;p&gt;\\u00a0Range is 0 - 32000&lt;\\/p&gt;</p>\r\n            ]]></Help>\r\n        </Parameter>\r\n\r\n        <Parameter>\r\n            <Index>92</Index>\r\n            <Label lang=\"en\">Minimum Change to send Report (%)</Label>\r\n            <Type>short</Type>\r\n            <Default>5</Default>\r\n            <Minim