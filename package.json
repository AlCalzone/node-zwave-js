{
  "name": "zwave-js",
  "version": "1.5.0",
  "description": "Z-Wave driver written entirely in JavaScript/TypeScript",
  "keywords": [],
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "files": [
    "build/**/*.js",
    "build/**/*.d.ts",
    "config/"
  ],
  "author": {
    "name": "AlCalzone",
    "email": "d.griesel@gmx.net"
  },
  "license": "MIT",
  "homepage": "https://github.com/AlCalzone/node-zwave-js#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AlCalzone/node-zwave-js.git"
  },
  "bugs": {
    "url": "https://github.com/AlCalzone/node-zwave-js/issues"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "devDependencies": {
    "@actions/core": "^1.0.0",
    "@actions/exec": "^1.0.0",
    "@actions/github": "^1.0.0",
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/plugin-proposal-numeric-separator": "^7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-typescript": "^7.3.3",
    "@commitlint/cli": "^8.0.0",
    "@commitlint/config-conventional": "^8.0.0",
    "@fimbul/mithotyn": "^0.21.0",
    "@fimbul/wotan": "^0.21.0",
    "@types/ansi-colors": "^3.2.0",
    "@types/debug": "4.1.5",
    "@types/fs-extra": "^8.0.0",
    "@types/jest": "^24.0.9",
    "@types/json5": "0.0.30",
    "@types/mockdate": "^2.0.0",
    "@types/moment-timezone": "^0.5.12",
    "@types/node": "^12.0.0",
    "@types/semver": "^6.0.0",
    "@types/serialport": "^7.0.1",
    "@types/triple-beam": "^1.3.0",
    "@types/yargs": "^13.0.0",
    "@typescript-eslint/eslint-plugin": "^1.6.0",
    "@typescript-eslint/parser": "^1.6.0",
    "clipboardy": "^2.1.0",
    "commitizen": "^4.0.3",
    "coveralls": "^3.0.3",
    "cz-conventional-changelog": "^3.0.2",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-prettier": "^3.0.1",
    "husky": "^3.0.0",
    "jest": "^24.1.0",
    "jest-circus": "^24.7.1",
    "jest-extended": "^0.11.1",
    "mockdate": "2.0.5",
    "prettier": "^1.17.0",
    "rimraf": "^3.0.0",
    "semver": "^6.0.0",
    "supports-color": "^7.0.0",
    "ts-node": "^8.0.2",
    "typescript": "3.7.0-dev.20191018",
    "yargs": "^14.0.0"
  },
  "dependencies": {
    "alcalzone-shared": "^2.0.0",
    "ansi-colors": "^4.0.1",
    "fs-extra": "^8.0.0",
    "json5": "^2.1.0",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.26",
    "reflect-metadata": "^0.1.13",
    "serialport": "^7.1.5",
    "winston": "^3.2.1"
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "watch": "npm run build -- --watch",
    "check": "npm run build -- --noEmit",
    "check:watch": "npm run check -- --watch",
    "generateCCAPIInterface": "ts-node maintenance/generateCCAPIInterface.ts",
    "cleanBuildDir": "rimraf ./build",
    "prebuild": "npm run generateCCAPIInterface",
    "prewatch": "npm run generateCCAPIInterface",
    "test:reset": "jest --clear-cache",
    "test:ts": "jest",
    "test:watch": "npm run test:ts -- --watch",
    "test": "npm run test:watch",
    "coverage": "npm run test:ts -- --collect-coverage",
    "coveralls": "npm run coverage && cat ./coverage/lcov.info | coveralls -v",
    "show-coverage": "npm run coverage && start ./coverage/index.html",
    "lint:ts": "eslint --ext .ts src",
    "lint:wotan": "wotan",
    "lint:config": "ts-node maintenance/lintConfigFiles.ts",
    "lint": "npm run lint:ts && npm run lint:wotan",
    "release": "ts-node maintenance/release.ts",
    "implemented_ccs": "ts-node maintenance/implemented_ccs.ts",
    "gh-cc-table": "npm run implemented_ccs -- --flavor=github --only-incomplete",
    "usbdebug": "cmd.exe /c \"test\\usbdebug.bat\"",
    "commit": "git-cz"
  },
  "readme": "README.md",
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
