        "1": {
            "label": "Default State",
            "description": "After Power-On-Reset of the device, the Channel Output State is set in Default State",
            "valueSize": 1,
            "minValue": 0,
            "maxValue": 2,
            "readOnly": false,
            "writeOnly": false,
            "allowManualEntry": false,
            "defaultValue": 2,
            "options": [
                {
                    "label": "Channel output state OFF",
                    "value": 0
                },
                {
                    "label": "Channel output state ON",
                    "value": 1
                },
                {
                    "label": "Channel output state remembers previous state",
                    "value": 2
                }
            ]
        },
        "3": {
            "label": "Follow State",
            "description": "This parameter allows Basic Set Commands to be sent when the channel output state changes (ON or OFF). Only the two least significant bits can be addressed.\n                Bit 0: Enable or disable Basic Set Commands sent to associated nodes of Group 2, when Channel Output State changes.\n                Bit 1: Enable or disable Basic Set Commands sent to associated nodes of Group 3, when Channel Output State changes.",
            "valueSize": 1,
            "minValue": 0,
            "maxValue": 3,
            "readOnly": false,
            "writeOnly": false,
            "allowManualEntry": true,
            "defaultValue": 3
        },
        "4": {
            "label": "Always ON",
            "description": "This parameter forces the channel output state to be always ON. It is not possible to turn OFF the channel output, which means that:\n                * All Switch OFF Command is rejected\n                * Basic Set OFF Command is rejected\n                * Binary Switch OFF Command is rejected\n                * Local Control does not toggle channel output state\n                * Product cannot be triggered by unsolicited Notification Report Command and enters in Alarm Mode",
            "valueSize": 1,
            "minValue": 0,
            "maxValue": 1,
            "readOnly": false,
            "writeOnly": false,
            "allowManualEntry": false,
            "defaultValue": 0,
            "options": [
                {
                    "label": "Channel output state always ON disable",
                    "value": 0
                },
                {
                    "label": "Channel output state always ON enable",
                    "value": 1
                }
            ]
        },
