			"minValue": 0,
			"maxValue": 2,
			"defaultValue": 1,
			"unsigned": true,
			"readOnly": false,
			"writeOnly": false,
			"allowManualEntry": false,
			"options": [
				{
					"label": "No dimming, only on/off (0/100%)",
					"value": 0
